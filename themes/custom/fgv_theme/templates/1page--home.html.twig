<script>

  $popup = '<img src="{{ imagem_popup }}" alt="" style="position:relative;right:5.5%;" class="desktop-only">';
  jQuery(document).ready(function() {
    var popup = "{{ imagem_popup }}";

  if ( window.location.href.includes("?page=") === false  && popup.length > 0 ) {
      setTimeout(function() {
    $('#lightbox > div').html($popup);
    $('#lightbox').addClass('ativo');
  }, 1000);
  }
});
$("#fechar").click(function(e) {
  e.preventDefault();
  $('#lightbox').removeClass('ativo video');
  $('#lightbox > div').html('');
});


  $popupmobile = '<img src="{{ imagem_popup }}" alt="" class="mobile-only" style="width: 300px;position: relative;right: 5%;">';
  jQuery(document).ready(function() {
    var popupmobile = "{{ imagem_popup }}";

