      setTimeout(function() {
    $('#lightbox-mobile > div').html($popupmobile);
    $('#lightbox-mobile').addClass('ativo');
  }, 1000);
  }
});
$("#fechar-mobile").click(function(e) {
  e.preventDefault();
  $('#lightbox-mobile').removeClass('ativo video');
  $('#lightbox-mobile > div').html('');
});
